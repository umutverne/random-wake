{
  "expo": {
    "name": "RandomWake",
    "slug": "random-wake",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/random-wake-logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "scheme": "randomwake",
    "splash": {
      "image": "./assets/random-wake-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1B1F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.randomwake.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptiveIcon.png",
        "backgroundColor": "#3C3C3C"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.randomwake.app",
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.USE_EXACT_ALARM",
        "android.permission.USE_FULL_SCREEN_INTENT",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "android": {
            "useNextNotificationChannelApi": true
          }
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2225480937157277~9269992452"
        }
      ],
      "expo-font",
      "./plugins/withAndroidAlarm"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5b519393-0182-45f6-bf54-9bf358b59ac6"
      }
    },
    "owner": "umutkilbizoglu"
  }
}
